    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
