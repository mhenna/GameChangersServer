    "supertest": "^2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "babel-polyfill"
    ],
    "plugins": [
      "add-module-exports",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ],
      [
        "transform-regenerator",
        {
          "asyncGenerators": true,
          "generators": true,
          "async": true
        }
      ],
      "babel-polyfill",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "transform": {
      ".": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "\\.test.js$",
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ]
  }
